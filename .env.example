# Copy this file to .env and fill in your secrets. DO NOT commit .env to git.

# MongoDB connection string
MONGODB_URI=mongodb+srv://<username>:<password>@cluster0.example.mongodb.net/eulaiq_outreach

# EulaIQ accounts - JSON list of {"identity":"...","password":"..."}
# Example: EULAIQ_ACCOUNTS=[{"identity":"a@company.com","password":"secret"}]
EULAIQ_ACCOUNTS=

# ZeptoMail SMTP accounts - JSON list
# Example: SMTP_ACCOUNTS=[{"email":"victor@eulaiq.com","username":"apikey","password":"secret"}, {"email":"victor@eulaiq.me","username":"apikey","password":"secret2"}]
SMTP_ACCOUNTS=

# YouTube channels for video uploads - JSON list of channel configs
# See docs/getYoutubeCredentials.md for setup instructions
# Each channel needs: channel_id, name, client_id, client_secret, refresh_token
# Example: YOUTUBE_CHANNELS=[{"channel_id":"UCxxx","name":"Channel 1","client_id":"xxx.apps.googleusercontent.com","client_secret":"xxx","refresh_token":"1//xxx","access_token":""}]
YOUTUBE_CHANNELS=

# Optional: Send test emails to this address during testing
TEST_EMAIL=chukwujiobivictoric@gmail.com

# AWS Bedrock (for LLM calls in step 2)
AWS_API_KEY=
AWS_BEDROCK_MODEL_ID=anthropic.claude-sonnet-4-5-20250929-v1:0
AWS_REGION=us-east-1
