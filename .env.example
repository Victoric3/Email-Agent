# Copy this file to .env and fill in your secrets. DO NOT commit .env to git.

# MongoDB connection string
MONGODB_URI=mongodb+srv://<username>:<password>@cluster0.example.mongodb.net/eulaiq_outreach

# EulaIQ accounts - JSON list of {"identity":"...","password":"..."}
# Example: EULAIQ_ACCOUNTS=[{"identity":"a@company.com","password":"secret"}]
EULAIQ_ACCOUNTS=

# ZeptoMail SMTP accounts - JSON list or multiple env vars
# Example: SMTP_ACCOUNTS=[{"email":"victor@eulaiq.com","username":"apikey","password":"secret"}, {"email":"victor@eulaiq.me","username":"apikey","password":"secret2"}]
SMTP_ACCOUNTS=

# Optional: Send test emails to this address during testing
TEST_EMAIL=chukwujiobivictoric@gmail.com

